<div class="main">
    <form id="searchForm">
        <h2>广告位分类详情</h2>
        <div id="addTypeForm" class="formBox">
            <div class="div-1">
                <div class="content">
                    <table class="c2" style="width:90%;">
                        <tr>
                            <th>名称：</th>
                            <td>
                                <span>$!adPositionType.name</span>
                            </td>
                        </tr>
                        <tr>
                            <th>编码：</th>
                            <td>
                                <span>$!adPositionType.code</span>
                            </td>
                        </tr>
                        <tr>
                            <th>描述：</th>
                            <td>
                                <span>$!{adPositionType.description}</span>
                            </td>
                        </tr>

                        <tr>
                            <th>创建时间：</th>
                            <td>
                                <span>$!{dateTool.format("yyyy-MM-dd HH:mm:ss",$!{adPositionType.gmtCreate})}</span>
                            </td>
                        </tr>
                        <tr>
                            <th>修改时间：</th>
                            <td>
                                <span>$!{dateTool.format("yyyy-MM-dd HH:mm:ss",$!{adPositionType.gmtModify})}</span>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div class="form-but"><!-- 表单按钮区 -->
            <input type="button" id="back" name="back" class="button-s4" value="返回" onclick="JavaScript:history.forward()">
        </div>
    </form>
</div>
