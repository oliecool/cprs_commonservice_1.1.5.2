<div class="basic">
    <ul class="form-tab">
        <li class="c">
            <span>操作提示</span>
        </li>
    </ul>
    <div class="formbox">
        <div class="clear"></div>
        <!-- 错误提示 -->
        <div class="nr error-box">
            <div class="nr-t"></div>
            <div class="nr-c" style="height:100px;">
                <div class="text">
                    <h4>#if($!message) $message #else 您无权查看该功能，请联系系统管理员！ #end</h4>
					<a   class="but" onclick="#if(!$!{url})javascript:history.go(-1)#else javascript: window.location.href='$!{appServer}$!{url}'#end" >返回</a>
				</div>
            </div>
            <div class="nr-b"></div>
        </div>
        <!-- 错误提示 END -->
        <div class="clear"></div>
    </div>
    <div class="clear"></div>
</div>
